# EditorConfig is awesome: https://EditorConfig.org
root = true

[*]
end_of_line = lf
insert_final_newline = true

[*.{ts, tsx}]
charset = utf-8
indent_style = space
indent_size = 2

[Makefile]
indent_style = tab
indent_size = 2

[{package.json,.docker-compose.yml}]
indent_style = space
indent_size = 2
