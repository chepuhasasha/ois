{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    // "rootDir": "./src/",
    "outDir": "./build/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "allowJs": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "declaration": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
